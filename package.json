{
  "name": "@polyhobbyist/babylon-collada-loader",
  "repository": {
    "type": "git",
    "url": "https://github.com/polyhobbyist/babylon-collada-loader.git"
  },
  "description": "Babylon.JS Loader for Collada",
  "version": "0.0.7",
  "main": "dist/dae.js",
  "types": "dist/dae.d.ts",
  "exports": {
    ".": {
      "types": "./dist/dae.d.ts",
      "require": "./dist/dae.js",
      "import": "./dist/dae.js"
    }
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "webpack --mode development && npm run generate-dts",
    "watch": "webpack --mode development --watch",
    "pretest": "npm run build",
    "prepublishOnly": "webpack --mode=production",
    "generate-dts": "rollup -c rollup.config.mjs"
  },
  "keywords": [
    "babylonjs",
    "ros",
    "ros2"
  ],
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  },
  "author": "lou@polyhobbyist.com",
  "license": "MIT",
  "dependencies": {
    "@types/xml2js": "^0.4.14",
    "babylonjs": "8.0.x",
    "babylonjs-gltf2interface": "8.0.x",
    "babylonjs-gui": "8.0.x",
    "babylonjs-loaders": "8.0.x",
    "babylonjs-materials": "8.0.x",
    "buffer": "^6.0.3",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "timers-browserify": "^2.0.12",
    "xml-loader": "^1.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "dts-bundle-generator": "^9.5.1",
    "dts-bundle-webpack": "^1.0.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rollup": "^4.37.0",
    "rollup-plugin-dts": "^6.2.1",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
